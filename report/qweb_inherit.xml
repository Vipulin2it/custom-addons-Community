<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="estate_property_report_inherit" inherit_id="sub_template_estate_property">
        <xpath expr="//div[@class='page']" position="replace">
            <div class="page">
                <h1>Inherited Sub Template</h1>
                <t t-set="a" t-value="45545"/>
                <div t-out="a"/>
                <h1 t-field="doc.name"/>
                <h1 t-field="doc.user_id"/>
                <div t-field="doc.postcode"/>
                <div t-field="doc.expected_price"/>
                <div t-field="doc.selling_price"/>
            </div>
        </xpath>
    </template>
</odoo>